<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <events>
      <event name="packageinstall:starting">
        <handler type="Sitecore.Support.PackagingEventHandler, Sitecore.Support.104141" method="OnPackageInstallSettingSwitcher" patch:before="*[@method='OnPackageInstallStartingHandler']" />
      </event>
      <event name="packageinstall:items:ended">
        <handler type="Sitecore.Support.PackagingEventHandler, Sitecore.Support.104141" method="OnPackageInstallDisposeSwitcher" patch:after="*[@method='OnPackageInstallItemsEndHandler']" />
      </event>
    </events>
  </sitecore>
</configuration>